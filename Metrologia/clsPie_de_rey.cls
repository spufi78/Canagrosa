'***************************************************************
'*   PROCEDIMIENTOS Y FUNCIONES DE LA CLASE CLSPIE_DE_REY
'***************************************************************
Public Function Carga(ID As String) As Boolean
        On Error GoTo fallo
        Dim rs As ADODB.Recordset
        Dim consulta As String
        consulta = "SELECT * FROM PIE_DE_REY WHERE EQUIPO_ID = " & ID
        Set rs = datos_bd(consulta)
        If rs.RecordCount = 0 Then
                Carga = False
        Else
                EQUIPO_ID = rs("EQUIPO_ID")
                NOMINAL_ANCHO_COMBINADO_L4 = rs("NOMINAL_ANCHO_COMBINADO_L4")
                TOLERANCIA_ANCHO_COMBINADO_L4_MM = rs("TOLERANCIA_ANCHO_COMBINADO_L4_MM")
                FRASE_CRITERIO_ACEPTACION_ESP = rs("FRASE_CRITERIO_ACEPTACION_ESP")
                FRASE_CRITERIO_ACEPTACION_ENG = rs("FRASE_CRITERIO_ACEPTACION_ENG")
                LONGITUD_BOCASH = rs("LONGITUD_BOCASH")
                LONGITUD_BOCAS_H = rs("LONGITUD_BOCAS_H")
                PUNTO_DE_CALIBRACION_N1_PALPADOR_DE_EXTERIORES = rs("PUNTO_DE_CALIBRACION_N1_PALPADOR_DE_EXTERIORES")
                PUNTO_DE_CALIBRACION_N2_PALPADOR_DE_EXTERIORES = rs("PUNTO_DE_CALIBRACION_N2_PALPADOR_DE_EXTERIORES")
                PUNTO_DE_CALIBRACION_N3_PALPADOR_DE_EXTERIORES = rs("PUNTO_DE_CALIBRACION_N3_PALPADOR_DE_EXTERIORES")
                PUNTO_DE_CALIBRACION_N4_PALPADOR_DE_EXTERIORES = rs("PUNTO_DE_CALIBRACION_N4_PALPADOR_DE_EXTERIORES")
